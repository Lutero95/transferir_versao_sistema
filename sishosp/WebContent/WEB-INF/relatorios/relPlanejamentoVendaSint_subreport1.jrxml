	<queryString>
		<![CDATA[select distinct codprocedimento,e.descricao as proced,
pvp.valor,
round(pvp.valor/pv.valortotal,2) perc,
pv.mes, pv.ano,pv.valortotal
  from clin.planejamento_venda pv
left join clin.planejamento_venda_detalhe_procedimento pvp on pvp.codplanejamento_venda = pv.codplanejamento_venda
join clin.estoque e on e.codprod = pvp.codprocedimento
