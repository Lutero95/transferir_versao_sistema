	<parameter name="codsubgrupo" class="java.lang.Integer"/>
	<parameter name="codangariador" class="java.lang.Integer"/>
	<parameter name="codvendedor" class="java.lang.Integer"/>
	<parameter name="codfilial" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="codindicador" class="java.lang.Integer">
