	</parameter>
	<queryString>
		<![CDATA[SELECT O1.codPRODUTO, E.DESCRICAO, SUM (O1.QUANTIDADE) AS QTDTOTAL,
SUM(O1.QUANTIDADE*O1.PRECOVENDA-coalesce(O.DESCONTO,0)/(select count(*) from clin.orcamento1 where codorcamento= o.codorcamento)) AS TOTALORCADO
      FROM clin.ORCAMENTO O
       JOIN clin.ORCAMENTO1 O1 ON O.codorcamento = O1.codORCAMENTO
       JOIN clin.ESTOQUE E ON O1.codPRODUTO = E.CODPROD
      WHERE O.DATA >=$P{datainicial}  AND O.DATA <= $P{datafinal}
and (( $P{coddivisao} >0 and e.coddivisao=$P{coddivisao})  or (($P{coddivisao} =0 ) or (($P{coddivisao} is null ))))
and (( $P{codsecao} >0 and e.codsecao=$P{codsecao}) or (($P{codsecao} =0 ) or (($P{codsecao} is null ))))
and (( $P{codgrupo} >0 and e.codgrupo=$P{codgrupo}) or (($P{codgrupo} =0 ) or (($P{codgrupo} is null ))))
and (( $P{codsubgrupo} >0 and e.codsubgrupo=$P{codsubgrupo}) or (($P{codsubgrupo} =0 ) or (($P{codsubgrupo} is null ))))

and (( $P{codangariador} >0 and o1.codangariador >0) or (($P{codangariador} =0 ) or (($P{codangariador} is null ))))


and (( $P{codvendedor} >0 and o.codvendedor  >0) or (($P{codvendedor} =0 ) or (($P{codvendedor} is null ))))
and (( $P{codindicador} >0 and o1.codindicador >0) or (($P{codindicador} =0 ) or (($P{codindicador} is null ))))
and (O.codfilial = $P{codfilial})

      GROUP BY O1.codPRODUTO, E.DESCRICAO
ORDER BY E.DESCRICAO]]>
	</queryString>
