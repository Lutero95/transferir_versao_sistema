	<queryString>
		<![CDATA[SELECT SUM(VLRVENDATOT) total, C.codcliente, C.NOME, coalesce(recibo.referente,'') referente,
recibo.dtemissao
 FROM CLIN.RECIBO
JOIN CLIN.VENDA1 V1 ON V1.CODVENDA = RECIBO.CODVENDA
JOIN CLIN.VENDA V ON V.CODVENDA = V1.CODVENDA
JOIN CLIN.CLIENTE C ON C.CODCLIENTE = V.codcliente
where recibo.codigo=$P{codrecibo}
GROUP BY C.CODCLIENTE, C.NOME, referente, recibo.dtemissao]]>
	</queryString>
	<field name="total" class="java.math.BigDecimal"/>
	<field name="codcliente" class="java.lang.Integer"/>
	<field name="nome" class="java.lang.String"/>
	<field name="referente" class="java.lang.String"/>
	<field name="dtemissao" class="java.sql.Date"/>
