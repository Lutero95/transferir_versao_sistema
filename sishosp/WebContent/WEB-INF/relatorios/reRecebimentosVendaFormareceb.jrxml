	<parameter name="codfilial" class="java.lang.Integer"/>
	<parameter name="codvendedor" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT CX.DT_ABERTURA,c.nome, V1.codANGARIADOR,V2.CODVENDA, V2.CODTIPPAG, TP.DESCRICAO,
SUM(V2.VALOR) AS TOTALRECEBIDO, V.CODCLIENTE
      FROM clin.VENDA2 V2
      LEFT JOIN clin.VENDA V ON V2.CODVENDA = V.CODVENDA
left  join clin.cliente c on c.codcliente = v.codcliente
      LEFT JOIN clin.VENDA1 V1 ON V2.CODVENDA = V1.CODVENDA and v1.ord=1
      LEFT JOIN clin.CAIXALOJA CX ON V2.SEQCAIXA = CX.CODCAIXALOJA
      LEFT JOIN clin.TIPPAG TP ON V2.CODTIPPAG = TP.CODTIPPAG
      WHERE CX.DT_ABERTURA >= $P{datainicial} AND CX.DT_ABERTURA <= $P{datafinal}
and v2.codfilial=$P{codfilial}
and (( $P{codcliente} is not null and v.codcliente =$P{codcliente}) or (($P{codcliente} =0 ) OR ($P{codcliente} is null )))
and (( $P{codvendedor} is not null and v.codvendedor =$P{codvendedor}) or (($P{codvendedor} =0 ) OR ($P{codvendedor} is null )))

and v.status<>'CAN'
and coalesce(v2.estornado,'N')<>'S'
      GROUP BY V2.CODTIPPAG, CX.DT_ABERTURA, V2.CODVENDA, TP.DESCRICAO, V.CODCLIENTE,V1.codANGARIADOR, c.nome
      order by tp.descricao,v2.codtippag,CX.DT_ABERTURA,c.nome]]>
	</queryString>
