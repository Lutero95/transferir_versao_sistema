	<parameter name="codcaixaloja" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select  distinct c.codcaixaloja,c.numcaixa, f.nome, c.saldoinicial, c.saldofinal,
c.dt_abertura, b.historico, tp.descricao, b.codvenda, v2.valor,v2.codvenda2,
cl.nome cliente
 from clin.caixaloja
c join clin.credcaixaloja b on
(c.codcaixaloja = b.codcaixaloja)

left join clin.venda  v on (b.codvenda = v.codvenda)
LEFT JOIN clin.cliente cl on cl.codcliente = v.codcliente
join clin.venda2 v2 on (v.codvenda = v2.codvenda) and v2.seqcaixa = c.codcaixaloja

left join clin.tippag tp on (v2.codtippag = tp.codtippag) join clin.func f on (c.codresponsavel = f.codfunc)
where c.codcaixaloja=$P{codcaixaloja}]]>
	</queryString>
