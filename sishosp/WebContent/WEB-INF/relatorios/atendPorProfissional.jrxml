	<queryString>
		<![CDATA[select m.id_medico cod_medico, m.descmedico nome_medico, e.descespecialidade nome_espec, a.dtaatende dt_atend, count(*) agendado,
COALESCE(atend.qtd,0) atend, coalesce(proffalta.qtd,0) proffalta,coalesce(pacfalta.qtd,0) pacfalta

from hosp.atendimentos a

left join (select aa.dtaatende, count(*) qtd, aa.codmedico from hosp.atendimentos aa
	join hosp.atendimentos1 a11 on a11.id_atendimento = aa.id_atendimento
	where aa.dtaatende between $P{dt_inicial} and $P{dt_final} and a11.situacao='A' group by aa.dtaatende, aa.codmedico) atend on (atend.codmedico = a.codmedico and a.dtaatende = atend.dtaatende)
left join (select aa.dtaatende, count(*) qtd, aa.codmedico from hosp.atendimentos aa
	join hosp.atendimentos1 a11 on a11.id_atendimento = aa.id_atendimento
	where aa.dtaatende between $P{dt_inicial} and $P{dt_final} and a11.situacao='MF' group by aa.dtaatende, aa.codmedico) proffalta on (proffalta.codmedico = a.codmedico and a.dtaatende = proffalta.dtaatende)
left join (select aa.dtaatende, count(*) qtd, aa.codmedico from hosp.atendimentos aa
	join hosp.atendimentos1 a11 on a11.id_atendimento = aa.id_atendimento
	where aa.dtaatende between $P{dt_inicial} and $P{dt_final} and a11.situacao is null group by aa.dtaatende, aa.codmedico) pacfalta on (pacfalta.codmedico = a.codmedico and a.dtaatende = pacfalta.dtaatende)
join hosp.medicos m on m.id_medico = a.codmedico
join hosp.especialidade e on m.id_medico = e.id_especialidade

where a.dtaatende between $P{dt_inicial} and $P{dt_final}
and a.codprograma = $P{cod_programa}
and a.codequipe = 0
and(  ($P{cod_medico} is not null and  m.id_medico = $P{cod_medico})
or   ($P{cod_medico} is null and  m.id_medico > 0))

group by  m.id_medico,  m.descmedico, e.descespecialidade, a.dtaatende, COALESCE(atend.qtd,0) , coalesce(proffalta.qtd,0) ,coalesce(pacfalta.qtd,0)

order by m.id_medico, a.dtaatende]]>
	</queryString>
