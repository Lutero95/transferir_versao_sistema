	<queryString>
		<![CDATA[select f.codfunc,f.nome,
pv.mes, pv.ano,pv.valortotal,PVC.codfunc, pvc.valor,
coalesce((select sum(vlrunit*qtdvda) from clin.venda1 v1 join clin.venda v on v.codvenda = v1.codvenda
where v.codvendedor=pvc.codfunc and v.dtemissao between to_date('01/'||pv.mes||'/'||pv.ano,'dd/mm/yyy') and
TO_DATE(cast(EXTRACT(DAY FROM ((pv.ano||'/'||(pv.mes)||'/01'):: DATE - 1)) as text)||'/'||pv.mes||'/'||pv.ano,'dd/mm/yyy')

),0) vlrvenda

  from clin.planejamento_venda pv
join clin.planejamento_venda_detalhe_colaborador pvc on pvc.codplanejamento_venda = pv.codplanejamento_venda
join clin.func f on f.codfunc = pvc.codfunc
where pv.mes=$P{mes} and ano=$P{ano}]]>
	</queryString>
