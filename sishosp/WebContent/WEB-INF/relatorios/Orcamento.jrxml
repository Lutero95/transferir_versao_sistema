	<parameter name="codorcamento" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select fi.logomarca,fi.descricao filial, fi.endereco||' '||fi.complemento endereco, fi.fone1,fi.fone2,cidade.descricao cidade,
 b.descricao bairro,
 estado.descricao estado, o.codorcamento,o.data, c.nome,f.nome as angariador,
o1.codproduto, e.descricao, o1.quantidade, o1.precovenda,
(o1.quantidade*o1.precovenda) total,
(select sum(orcamento1.quantidade*orcamento1.precovenda) from clin.orcamento1 where orcamento1.codorcamento=o.codorcamento)  totalorc,
(select sum(orcamento1.quantidade*orcamento1.precovenda) from clin.orcamento1 where orcamento1.codorcamento=o.codorcamento) - o.desconto vlrliquido,
o.desconto,
validadeorcamento
 from clin.orcamento o
join clin.orcamento1 o1 on o1.codorcamento = o.codorcamento
join clin.cliente c on c.codcliente = o.codcliente
left join clin.func f on f.codfunc = o.codangariador
JOIN clin.estoque e on e.codprod = o1.codproduto
join clin.filial fi on fi.codfilial = o.codfilial
left join clin.cidade on cidade.codcidade = fi.codcidade
left join clin.estado on estado.codestado = fi.codestado
left join clin.bairro b on b.codbairro = fi.codbairro
 join clin.parametros on parametros.codfilial=o.codfilial
where o.codorcamento=$P{codorcamento}
group by
o.codorcamento,o.data, c.nome,f.nome ,
o1.codproduto, e.descricao, o1.quantidade, o1.precovenda,
(o1.quantidade*o1.precovenda) ,
o.desconto,
validadeorcamento,
fi.descricao, fi.endereco||' '||fi.complemento, fi.fone1,fi.fone2,
cidade.descricao, estado.descricao,b.descricao, fi.logomarca]]>
	</queryString>
