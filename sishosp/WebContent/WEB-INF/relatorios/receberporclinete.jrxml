	<queryString>
		<![CDATA[select r.data_emissao, r.competencia,r.valor ,r.desconto ,r.vencimento ,r.status , c.nome,p.descricao from clin.recdup
	r join clin.cliente c on (r.id_cliente = c.codcliente)
	join clin.portador p on (r.id_portador = p.codportador)

where r.id_cliente =$P{codcliente}order by c.nome;]]>
